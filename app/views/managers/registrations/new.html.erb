<div class="row wrapper border-bottom white-bg page-heading">
  <div class="col-lg-10">
    <h2><%= "注册" %></h2>
  </div>
</div>
<div class="wrapper wrapper-content animated fadeInRight">
  <%#= form_for @role, html:{class: "form-horizontal", id: "new_role_form"} do |f| %>
  <%= form_for(resource, as: resource_name, url: registration_path(resource_name), :html => {class: "form-horizontal", id: "new_role_form"}) do |f| %>
  <%= devise_error_messages! %>
    <div class="row">
      <div class="col-lg-12">
        <div class="ibox float-e-margins">
          <div class="ibox-content">
            <div class="form-group" style="margin-top: 20px"><label class="col-sm-1 control-label">邮箱</label>
              <div class="col-sm-10">
                <%= f.text_field :email, autofocus: true, class: "form-control" %>
              </div>
            </div>

            <div class="hr-line-dashed"></div>
            <div class="form-group"><label class="col-sm-1 control-label">电话</label>
              <div class="col-sm-10">
                <%= f.text_field :phone, class: "form-control" %>
              </div>
            </div>

            <div class="hr-line-dashed"></div>
            <div class="form-group"><label class="col-sm-1 control-label">密码</label>
              <div class="col-sm-10">
                <%= f.text_field :password, autocomplete: "off", id: 'password', class: "form-control" %>
              </div>
            </div>

            <div class="hr-line-dashed"></div>
            <div class="form-group"><label class="col-sm-1 control-label">确认密码</label>
              <div class="col-sm-10">
                <%= f.text_field :password_confirmation, autocomplete: "off", id: 'confirm', class: "form-control" %>
              </div>
            </div>
            
            <div class="hr-line-dashed"></div>
            <div class="form-group">
              <div class="col-sm-4 col-sm-offset-1">
              <%= f.submit t("public.submit"), class: 'btn btn-primary' %>
              <%= link_to t("public.cancel"), :back , class: 'btn btn-default' %>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  <%end%>
  <div class="divider"></div>
</div>



