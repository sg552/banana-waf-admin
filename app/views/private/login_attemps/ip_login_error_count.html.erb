<div class="row white-bg page-heading">
  <div class="col-lg-12">
    <h2>IP与密码错误数量表</h2>
  </div>
</div>

<div class="wrapper wrapper-content animated fadeInRight ecommerce">
  <div class="row">
    <div class="col-lg-12">
      <div class="ibox">
        <div class="ibox-content">
          <tfoot>
            <tr>
              <th colspan="19">
                <ul class="pull-right"><%= paginate @ip_error_count %></ul>
              </th>
            </tr>
          </tfoot>
          <div><h3><%= @ip_error_count.size %>条数据<h3></div>
          <table class="table-hover table table-stripped toggle-arrow-tiny">
            <thead>
              <th>IP</th>
              <th>错误次数</th>
            </thead>
            <tbody>
              <% @ip_error_count.each do |ip_error_count| %>
                <tr>
                  <td><%= link_to ip_error_count[:ip], ip_statistical_analysis_user_behaviers_path(ip: ip_error_count[:ip])%></td>
                  <td>
                    <%= ip_error_count[:count] %>
                  </td>
                </tr>
              <% end %>
            </tbody>
            <tfoot>
              <tr>
                <th colspan="19">
                  <ul class="pull-right"><%= paginate @ip_error_count %></ul>
                </th>
              </tr>
            </tfoot>
           
          </table>
        </div>
      </div>
    </div>
  </div>
</div>

<script type="text/javascript">
  $('form').validate()
  $('.datepicker').datetimepicker({
    format: "Y-m-d H:i"
  })
</script>